# ios/Gemfile
source "https://rubygems.org"

# CocoaPods is fine on 1.16.x, but xcodeproj must come from Git (supports objectVersion 70)
gem "cocoapods", ">= 1.16.2"

# Pull xcodeproj straight from GitHub master (or pin a commit)
gem "xcodeproj", git: "https://github.com/CocoaPods/Xcodeproj", branch: "master"